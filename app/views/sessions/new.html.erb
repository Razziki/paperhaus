<h1>Log in</h1>

<%= form_with url: sessions_path, method: :post, local: true do |f| %>
  <div>
    <%= label_tag :email %><br>
    <%= text_field_tag :email %>
  </div>

  <div>
    <%= label_tag :password %><br>
    <%= password_field_tag :password %>
  </div>

  <div>
    <%= f.submit "Log in" %>
  </div>
<% end %>
